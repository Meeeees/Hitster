{
  "version": 2,
  "builds": [
    {
      "src": "/*.html",
      "use": "@vercel/static"
    },
    {
      "src": "/login",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/login",
      "dest": "/login"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
